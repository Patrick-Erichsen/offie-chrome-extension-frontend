{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "strict": true,
        "noImplicitReturns": true,
        "noImplicitAny": true,
        "module": "es6",
        "moduleResolution": "node",
        "target": "es5",
        "allowJs": true,
        "jsx": "react-jsx",
        "allowSyntheticDefaultImports": true,
        "lib": ["DOM", "DOM.Iterable", "ES6"]
    },
    "include": [
        "./src/**/*",
    ]
}